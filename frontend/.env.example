# Example frontend environment variables for sat_tracker
# Copy this file to `.env` and replace placeholders before running.

# Cesium Ion access token (required for Cesium resources)
VITE_CESIUM_ION_ACCESS_TOKEN=REPLACE_WITH_YOUR_CESIUM_ION_ACCESS_TOKEN

# Backend API base URL used by the frontend to fetch TLEs
# e.g. http://localhost:5000 when backend runs locally
VITE_API_BASE_URL=http://localhost:5000
